# Sprint Flight Application Environment Variables
# Copy this file to .env and update with your actual API keys

# AviationStack API Configuration
FLIGHT_API_KEY=your_aviationstack_api_key_here
FLIGHT_API_BASE_URL=http://api.aviationstack.com/v1

# Gemini AI Configuration (Optional - fallback responses work without this)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash

# Application Configuration
SERVER_PORT=8080
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_APP=DEBUG

# IMPORTANT: Never commit the actual .env file to version control!
# This file contains your sensitive API keys.
